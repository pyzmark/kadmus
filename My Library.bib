@article{zwierlin-diehl_phrygillos:_1992,
	title = {Phrygillos: {Zum} {Problem} der {Identität} des {Gemmenschneiders} und des {Münzstempelschneiders}},
	volume = {35},
	issn = {0003-5688},
	shorttitle = {{PHRYGILLOS}},
	url = {http://www.jstor.org/stable/41320968},
	abstract = {Le graveur de gemmes Phrygillos (Φρύγιλλος) était connu par des moulages d'une cornaline perdue représentant un Eros jouant sur le sol. La pierre, vraisemblablement un scarabéoïde, avait passé de la collection romaine de Francesco Vettori (1710-1770) à celle du Duc de Blacas (1770-1839). Cependant, elle ne figurait pas au nombre des pièces de la collection Blacas acquises en 1867 par le British Museum. Pour la connaissance de cette pièce, le moulage de l'empreinte de verre non retouchée de la Collection Stosch peut faire office d'original (pl. 22, 1.2). En plus d'un scarabéoïde en cornaline de Boston, attribué au même Phrygillos (pl. 22, 3), on connaît une deuxième pierre signée de son nom. Il s'agit d'un scarabéoïde en calcédoine d'une collection privée, représentant Héraklès s'emparant du trépied delphique. Cette pièce a été publiée par G. Dembski, il y a dix ans, sans éveiller, apparemment, l'attention des spécialistes. Grâce à l'obligeance du propriétaire et par l'entremise de G. Dembski, l'auteur a pu étudier l'original. De nouvelles prises de vues de l'original et du moulage sont publiées dans cette étude (pl. 22, 4-8). La figure archaïsante de l'Héraklès est tirée d'un groupe archaïque représentant la lutte pour le trépied, groupe reproduit sur un scarabée de Boston du dernier quart du VIe siècle (pl. 22, 9). Le modèle archaïque sera repris, par le biais d'un support intermédiaire, sur des reliefs néoattiques et romains impériaux. Malgré la différence des motifs, on reconnaît la main du même maître sur la pierre gravée représentant Eros et sur celle d'Héraklès, aussi bien dans le détail de la décoration que dans la manière de signer. La comparaison des pierres gravées avec les monnaies de Syracuse signées ΦΡΥΓΙΛΛΟΣ et ΦΡΥ (pl. 23, 1.2a) est plus difficile, à cause de la différence de nature des objets, de l'éventail distinct des motifs et des écarts de formats. Cependant, certaines particularités, notamment le profil caractéristique des visages, dotés d'un nez proéminent et d'un œil globuleux, apportent la preuve de l'identité du graveur de gemmes et du graveur de coins. Des signes distinctifs analogues permettent d'attribuer au même maître les statères et distatères de Thurioi signés de ΦΡΥ, Φ, et de l'oiseau φρυγίλος, symbole évident (pl. 23, 3-6). Les hypothèses avancées autrefois sur l'origine de Phrygillos, basées sur son nom, ne peuvent être acceptées pour des raisons philologiques. En se basant sur l'analyse du style, on peut dire que le tailleur de gemmes est issu d'une école de la Grèce de l'Est. Les autres monnaies siciliennes et d'Italie du Sud (de Terina, Leontinoi, Velia/Hyele, Hérakléia, Pandosia) figurant le Φ ne révèlent pas d'élément caractéristique de Phrygillos.},
	language = {German},
	number = {2},
	urldate = {2018-07-24},
	journal = {Antike Kunst},
	author = {Zwierlin-Diehl, Erika},
	year = {1992},
	pages = {106--117},
}

