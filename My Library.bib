@article{windhager_exhibiting_2019,
	title = {Exhibiting {Uncertainty}: {Visualizing} {Data} {Quality} {Indicators} for {Cultural} {Collections}},
	volume = {6},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	shorttitle = {Exhibiting {Uncertainty}},
	url = {https://www.mdpi.com/2227-9709/6/3/29},
	doi = {10.3390/informatics6030029},
	abstract = {Uncertainty is a standard condition under which large parts of art-historical and curatorial knowledge creation and communication are operating. In contrast to standard levels of data quality in non-historical research domains, historical object and knowledge collections contain substantial amounts of uncertain, ambiguous, contested, or plainly missing data. Visualization approaches and interfaces to cultural collections have started to represent data quality and uncertainty metrics, yet all existing work is limited to representations for isolated metadata dimensions only. With this article, we advocate for a more systematic, synoptic and self-conscious approach to uncertainty visualization for cultural collections. We introduce omnipresent types of data uncertainty and discuss reasons for their frequent omission by interfaces for galleries, libraries, archives and museums. On this basis we argue for a coordinated counter strategy for uncertainty visualization in this field, which will also raise the efforts going into complex interface design and conceptualization. Building on the PolyCube framework for collection visualization, we showcase how multiple uncertainty representation techniques can be assessed and coordinated in a multi-perspective environment. As for an outlook, we reflect on both the strengths and limitations of making the actual wealth of data quality questions transparent with regard to different target and user groups.},
	language = {en},
	number = {3},
	urldate = {2021-09-15},
	journal = {Informatics},
	author = {Windhager, Florian and Salisu, Saminu and Mayr, Eva},
	month = sep,
	year = {2019},
	note = {Number: 3
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {cognition support, cultural collections, data quality, digital humanities, graphs, maps, sets, time-oriented data, uncertainty, visualization},
	pages = {29},
	file = {Full Text PDF:/Users/mpyzyk/Zotero/storage/XW7QG4A2/Windhager et al. - 2019 - Exhibiting Uncertainty Visualizing Data Quality I.pdf:application/pdf;Snapshot:/Users/mpyzyk/Zotero/storage/38TUMDLV/29.html:text/html},
